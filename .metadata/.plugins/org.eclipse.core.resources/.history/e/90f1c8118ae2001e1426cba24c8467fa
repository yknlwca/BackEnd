package com.ssafy.fit.model.repository;

import java.util.List;

import com.ssafy.fit.model.dto.Review;
import com.ssafy.fit.model.dto.Video;

public interface ReviewRepository {
	public abstract List<Review> selectAll();

	public abstract Review selectOne(int id);

	public abstract void insertBoard(Review review);

	public abstract void updateBoard(Review review);

	public abstract void deleteBoard(int id);

	public abstract void updateViewCnt(int id);
}
