package com.ssafy.fit.model.service;

import java.util.List;

import com.ssafy.fit.model.dto.Review;
import com.ssafy.fit.model.dto.Video;

public interface ReviewService {

	public abstract List<Review> getList();

	public abstract Review getBoard(int id);

	public abstract void writeBorad(Review review);

	public abstract void modifyBoard(Review review);

	public abstract void removeBoard(int id);
}
